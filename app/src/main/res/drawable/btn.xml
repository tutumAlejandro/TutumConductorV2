<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="150dp"/>
            <gradient
                android:type="linear"
                android:startColor="@color/Yelowl"
                android:centerColor="@color/Yelowh"
                android:endColor="@color/Yelowh"
                android:angle="270">
            </gradient>
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="150dp"/>
            <gradient
                android:type="linear"
                android:startColor="@color/Yelowl"
                android:centerColor="@color/Yelowl"
                android:endColor="@color/Yelowl"
                android:angle="270">
            </gradient>
        </shape>
    </item>


</selector>

